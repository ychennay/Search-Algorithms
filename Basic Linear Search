public class App {

    public static void main(String[] args){

        int[] nums = new int[5];

        for (int i=1; i<nums.length; i++)
            nums[i] = i * 49;

        int num = nums[4];

        for (int i=0; i<nums.length; ++i)
            if (nums[i] == num)
                System.out.println("Index found: " + i);
                System.out.println("Value: " + num);
    }
}
